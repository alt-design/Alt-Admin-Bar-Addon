{
    "name": "alt-design/alt-admin-bar",
    "description": "Alt Admin Bar Addon",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AltDesign\\AltAdminBar\\": "src"
        }
    },
    "require": {
        "php": "^8.1",
        "statamic/cms": "^4.0|^5.0"
    },
    "extra": {
        "statamic": {
            "name": "Alt Admin Bar",
            "description": "Alt Admin Bar addon"
        },
        "laravel": {
            "providers": [
                "AltDesign\\AltAdminBar\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": false,
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "pestphp/pest": "^2.35",
        "pestphp/pest-plugin-laravel": "^2.4",
        "mockery/mockery": "^1.6"
    }
}
